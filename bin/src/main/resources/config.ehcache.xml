<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.ehcache.org/ehcache.xsd"
         updateCheck="true" monitoring="autodetect" dynamicConfig="true">

	
    <cache name="cache-1min" 	timeToLiveSeconds="60" 		maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>    
    <cache name="cache-5min" 	timeToLiveSeconds="300"  	maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>    
    <cache name="cache-30min"	timeToLiveSeconds="1800" 	maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    <cache name="cache-1hour"	timeToLiveSeconds="3600" 	maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>    
    <cache name="cache-1day"	timeToLiveSeconds="86400"	maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    <cache name="cache-7day" 	timeToLiveSeconds="604800" 	maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    <cache name="cache-10day"	timeToLiveSeconds="864000"  maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    <cache name="cache-15day"	timeToLiveSeconds="1296000" maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    <cache name="cache-30day"  	timeToLiveSeconds="2592000" maxElementsInMemory="50" eternal="false" overflowToDisk="false" timeToIdleSeconds="0" memoryStoreEvictionPolicy="LFU" transactionalMode="off"></cache>
    
</ehcache>